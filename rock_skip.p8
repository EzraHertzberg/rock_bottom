function shop_tile(x, y, sprs)
	add(shop_tiles,{
	x = x,
	y = y,
	sprs = sprs,
	hover = false,
	bought = false
	})
end

--note the outrageous lack of 
--for loop
function draw_tile(tile)
if not tile.bought then
	for i = 1, 2 do
		spr(tile.sprs[1], tile.x - 16 + (i*16), tile.y, 2,2)
	end
	if not tile.hover then
		spr(tile.sprs[2], tile.x + 32, tile.y, 2,2)	
	else
		spr(tile.sprs[1], tile.x + 32, tile.y, 2,2)
		spr(tile.sprs[2], tile.x + 48, tile.y, 2,2)	
	end
	else
	for i = 1, 2 do
		spr(tile.sprs[3], tile.x - 16 + (i*16), tile.y, 2,2)
	end
		if not tile.hover then
		spr(tile.sprs[4], tile.x + 32, tile.y, 2,2)	
	else
		spr(tile.sprs[3], tile.x + 32, tile.y, 2,2)
		spr(tile.sprs[4], tile.x + 48, tile.y, 2,2)	
	end
end
end
