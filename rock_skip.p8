		end	
	end
	
	if btn(3) and button_cooldown == 0 then
		button_cooldown = 5
		if hovered_t == 3 then
			hovered_t = 1
		else 
			hovered_t = hovered_t + 1
		end
	end
	
	if btn(5)
	and xp >= goods[hovered_t][2] and
	goods[hovered_t][1] == "not purchased"
	then
