function lose()
	print("your rock sank, press z\nto restart")
	print("you made it " .. flr(rock.x).." feet\nand earned "..flr(rock.x)\5  .." xp")
end


--checks for rectangle rectangle collision between rock and object
function collide(obj)
	if rock.x + rock.w > obj.x and
	rock.x < obj.x + obj.w and
	rock.y + rock.h > obj.y and
	rock.y < obj.y + obj.h then
			return true
	end


end 
