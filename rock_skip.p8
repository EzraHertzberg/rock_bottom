--super janky boolean to
--see if your shopping
shop = false

function _init()
--adds obsticals to dictionary
	for i = 1, 28 do
	--flying birds
		add_obs( i * 128, 64,
	 7, 4, {7, 8}, 2)
	end
	
	for i = 1, 40 do
		add_obs( i * 128, 116,
	 5, 6, 5, 2) 
	 
	end
--sets up rock
	rock_set()
end


function _update()
		
		if super_begining then
		if btn(5) then 
		super_begining = false
		home = true
		end
		else
		if button_cooldown > 0 then
		button_cooldown = 
		button_cooldown - 1
		end

		if playing then
		home = false
		current_frame = current_frame + 1
		rock_update()
		--makes rock accelerate down
		if btn(3) then
			rock_dive()
		end
		--makes rock accelerate up if user presses down when rock is touching water
		if btn(2)  and rock.y > 14*8 and rock.y < 16*8 then
			rock_skip()
		end
	--resets the variables of the rock if the player restarts the game	
		if btn(4) and rock.sunk then
			playing = false
			rock_set()
			home = true
			button_cooldown = 10
			camera(0,0)
		end
	
	--makes the rock sink
		if rock.y > 8 * 16 then
			rock_sink()
		end
		--animates birds and checks collision
		for i = 1, #obsticals do
				if collide(obsticals[i]) and 
				rock.cooldown == 0 then
					rock.dy = 
					obsticals[i].effect
					rock.dx +=
					obsticals[i].effect / 5 
					rock.cooldown = 5		
				end
		end		
	
		else	

	
			if button_cooldown == 0 and btn(4) and home then
				if not shop then
				button_cooldown = 10
				end
				shop = true
			elseif btn(5) and home then
			playing = true
			end
	end
end
end
