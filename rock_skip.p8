--adds obstical dictionary
function add_obs(x, y, w, h,
 sprite, effect)
	add(obsticals, {x = x,
	 y = y,
	 sprite = sprite,
	 cur_spr = 1,
	 w = w,
	 h = h,
	 effect = effect})
end

function buy_obs(obs)
	obs.effect = effect * -1
end


--draws the obs at a location
function draw_obs(obs)
	if type(obs.sprite) == "number" then
		spr(obs.sprite, obs.x, obs.y) 
	elseif type(obs.sprite) == "table" then
		if  current_frame % 15 == 0
		 then
			if obs.cur_spr < #obs.sprite then
				obs.cur_spr =
			 obs.cur_spr + 1
			else 
				obs.cur_spr = 1
			end	
		end
	spr(obs.sprite[obs.cur_spr], 
		obs.x, obs.y)			
	end
end

