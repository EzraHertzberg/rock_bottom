--resets the rock variables
--useful for restarting game
function rock_set()
	rock.sprite = 1
	rock.x = 0
	rock.y = 80
	rock.w = 8
	rock.h = 4
	rock.dx = 2
	rock.dy = 3
	rock.skipped = false
	rock.cooldown = 0
	rock.sunk = false
	rock.dived = false
end
--bounces the rock on water
--slowly will make rock sink
--if player doesn't get speed
--boost
function rock_skip()
	rock.dx = rock.dx * 0.95
	rock.dy = rock.dx * 2
	rock.dived = false
end
--does the rock dive move
function rock_dive()
	if not rock.dived then 
		rock.dy = rock.dy - 2
		rock.dived = true
	end
end

--moves the rock
function rock_update()
	if not rock.sunk then
		rock.dy = rock.dy - gravity
		rock.x = rock.x + rock.dx
		rock.y = rock.y - rock.dy
		if rock.cooldown > 0 then
			rock.cooldown =
			rock.cooldown - 1
		end
	end
end
--draws the rock as a splash
--of water if it sinks
function rock_sink()
	rock.sunk = true
	rock.sprite = 6
	rock.y = 8 * 13
end

